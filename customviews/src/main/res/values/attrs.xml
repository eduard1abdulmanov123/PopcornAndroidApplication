<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FlowLayout">
        <attr name="max_line" format="integer"/>
        <attr name="indents_between_elements" format="dimension"/>
        <attr name="gravity" format="enum">
